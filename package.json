  },
  "version": "1.0.0",
  "scripts": {
    "start": "yarn build && yarn preview",
    "dev": "vite",
    "build": "tsc && vite build --base=./",
    "preview": "vite preview",
    "test": "jest",
    "coverage": "jest --coverage --env=jsdom",
    "test:watch": "jest --watch",
    "test:findRelatedTests": "jest --findRelatedTests --passWithNoTests",
    "ts:check": "tsc",
    "lint:findCyclicDeps": "madge ./src -c --extensions ts,tsx",
    "lint:css": "stylelint 'src/**/*.{jsx,js,tsx,ts}'",
    "lint:eslint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint": "yarn ts:check && yarn lint:eslint && yarn lint:css && yarn lint:findCyclicDeps",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
